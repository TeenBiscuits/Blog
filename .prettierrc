{
	"semi": false,
	"singleQuote": true,
	"useTabs": true,
	"tabWidth": 2,
	"trailingComma": "es5",
	"printWidth": 80,
	"plugins": [
		"prettier-plugin-organize-imports",
		"prettier-plugin-astro",
		"prettier-plugin-tailwindcss"
	],
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		}
	]
}
