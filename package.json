{
	"name": "blog",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "bunx --bun astro dev",
		"start": "bunx --bun astro dev",
		"prebuild": "bun src/plugins/indexnow.ts",
		"build": "bunx --bun astro check && bunx --bun astro build",
		"preview": "bunx --bun astro preview",
		"astro": "bunx --bun astro",
		"lint": "bunx --bun eslint . --ext .js,.mjs,.jsx,.ts,.tsx,.astro",
		"lint:fix": "bunx --bun eslint . --ext .js,.mjs,.jsx,.ts,.tsx,.astro --fix",
		"format": "bunx --bun prettier --write .",
		"format:check": "bunx --bun prettier --check ."
	},
	"dependencies": {
		"@astrojs/check": "^0.9.5",
		"@astrojs/mdx": "^4.3.10",
		"@astrojs/rss": "^4.0.13",
		"@astrojs/sitemap": "^3.6.0",
		"@astrojs/vercel": "^9.0.0",
		"@fontsource-variable/cascadia-code": "^5.2.2",
		"@fontsource-variable/eb-garamond": "^5.2.7",
		"@fontsource-variable/inter": "^5.2.8",
		"@fontsource-variable/jetbrains-mono": "^5.2.8",
		"@fontsource-variable/merriweather": "^5.2.6",
		"@fontsource-variable/rubik": "^5.2.8",
		"@fontsource/monaspace-neon": "^5.2.5",
		"@fontsource/silkscreen": "^5.2.8",
		"@iconify-json/mdi": "^1.2.3",
		"@tailwindcss/vite": "^4.1.17",
		"astro": "^5.15.6",
		"astro-icon": "^1.1.5",
		"astro-og-canvas": "^0.7.2",
		"dayjs": "^1.11.19",
		"dotenv": "^17.2.3",
		"mdast-util-to-string": "^4.0.0",
		"reading-time": "^1.5.0",
		"rehype-figure-title": "^1.0.0",
		"rehype-katex": "^7.0.1",
		"remark-math": "^6.0.0",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"unist-util-visit": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"@tailwindcss/typography": "^0.5.19",
		"@types/bun": "^1.3.2",
		"@typescript-eslint/eslint-plugin": "^8.46.4",
		"@typescript-eslint/parser": "^8.46.4",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-astro": "^1.5.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"prettier": "^3.6.2",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-organize-imports": "^4.3.0",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"rehype-accessible-emojis": "^0.3.2",
		"typescript-eslint": "^8.46.4"
	}
