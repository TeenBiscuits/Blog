		"@fontsource-variable/jetbrains-mono": "^5.2.8",
		"@fontsource/silkscreen": "^5.2.8",
		"@iconify-json/mdi": "^1.2.3",
		"@tailwindcss/vite": "^4.1.14",
		"astro": "^5.14.4",
		"astro-icon": "^1.1.5",
		"dayjs": "^1.11.18",
